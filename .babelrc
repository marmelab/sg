{
    "env": {
        "dev": {
            "plugins": [
                "transform-async-to-generator",
                "transform-object-rest-spread",
                ["babel-plugin-transform-es2015-modules-commonjs"]
            ]
        },
        "dist": {
            "presets": ["es2015", "es2016", "stage-2"],
            "plugins": [
                "add-module-exports"
            ]
        }
    }
}
